{
  "name": "mz-bulma-jquery-electron-template",
  "version": "1.0.0",
  "main": "app/main.js",
  "scripts": {
    "dev": "concurrently \"yarn serve\" \"yarn app\"",
    "serve": "browser-sync start --config bs-config.js",
    "app": "wait-on tcp:3000 && nodemon --config nodemon.json --exec \"electron .\"",
    "electron": "electron ."
  },
  "license": "MIT",
  "dependencies": {
    "electron-window-state": "^5.0.3",
    "jquery": "^3.6.0",
    "query-string": "^7.1.1"
  },
  "devDependencies": {
    "browser-sync": "^2.27.9",
    "concurrently": "^7.0.0",
    "electron": "^17.1.2",
    "electron-is-dev": "^2.0.0",
    "nodemon": "^2.0.15",
    "wait-on": "^6.0.1"
  }
}
